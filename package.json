{
  "name": "rivet-plugin-transformerlab",
  "packageManager": "yarn@3.5.0",
  "version": "0.0.6",
  "type": "module",
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && tsx bundle.ts",
    "dev": "run-p watch:tsc watch:esbuild:sync",
    "watch:tsc": "tsc -b -w --preserveWatchOutput",
    "watch:esbuild": "tsx bundle.ts --watch",
    "watch:esbuild:sync": "tsx bundle.ts --watch --sync",
    "publish": "yarn npm publish --access public"
  },
  "dependencies": {
    "@ironclad/rivet-core": "^1.4.0",
    "axios": "^1.6.7",
    "got": "^14.2.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.6",
    "p-retry": "^6.2.0",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "recursive-copy": "^2.0.14",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.6.1"
  },
  "rivet": {
    "skipInstall": true
  }
}
